{"ast":null,"code":"import axios from \"axios\";\nimport { useModal } from 'vuestic-ui';\nexport default {\n  name: 'ListTodo',\n  setup() {\n    const {\n      confirm\n    } = useModal();\n    const onButtonClick = () => {\n      confirm('Are you sure you want to delete this item?').then(ok => ok && console.log(this.selectedId));\n    };\n  },\n  data() {\n    return {\n      selectedId: 0,\n      todos: {},\n      next: \"\",\n      prev: \"\",\n      link: \"http://127.0.0.1:8000/api/v1/todo-list/\"\n    };\n  },\n  methods: {\n    deleteIt() {\n      axios.delete(\"http://127.0.0.1:8000/api/v1/todo-list/delete/\" + this.selectedId + \"/\").then(alert(\"deleted\")).catch(err => {\n        alert(\"error\"), console.log(err.response);\n      });\n    },\n    setNext() {\n      this.next = this.todos['next'];\n      console.log(this.next);\n      axios.get(this.next).then(response => {\n        this.todos = response.data;\n      }).catch(err => {\n        console.log(err.response);\n      });\n    },\n    setPrev() {\n      this.prev = this.todos['previous'];\n      axios.get(this.prev).then(response => {\n        this.todos = response.data;\n      }).catch(err => {\n        console.log(err.response);\n      });\n    }\n  },\n  computed: {\n    selectedOrder() {\n      return this.$store.state.selectedOrder;\n    },\n    selectedStatus() {\n      return this.$store.state.selectedStatus;\n    },\n    selectedID() {\n      return this.$store.state.selectedID;\n    }\n  },\n  created: function () {\n    if (this.selectedOrder != \"\") {\n      this.link = \"http://127.0.0.1:8000/api/v1/todo-list/\" + this.$store.state.selectedOrder + \"/\";\n    } else {\n      this.link = \"http://127.0.0.1:8000/api/v1/todo-list/\";\n    }\n    axios.get(this.link).then(response => {\n      this.todos = response.data;\n    }).catch(err => {\n      alert(\"error fetching list\"), console.log(err);\n    });\n  },\n  watch: {\n    'selectedOrder': function () {\n      if (this.selectedOrder != \"\") {\n        if (this.link.includes(\"asc\")) this.link = this.link.replace(\"asc\", \"desc\");else if (this.link.includes(\"desc\")) this.link = this.link.replace(\"desc\", \"asc\");else this.link = this.link + this.$store.state.selectedOrder + \"/\";\n      } else {\n        this.link = \"http://127.0.0.1:8000/api/v1/todo-list/\";\n      }\n      axios.get(this.link).then(response => {\n        this.todos = response.data;\n      }).catch(err => {\n        alert(\"error fetching list\"), console.log(err);\n      });\n    },\n    'selectedStatus': function () {\n      if (this.selectedStatus != \"\") {\n        if (this.$store.state.selectedOrder != \"\") this.link = \"http://127.0.0.1:8000/api/v1/todo-list/\" + this.$store.state.selectedStatus + \"/\" + this.$store.state.selectedOrder + \"/\";else this.link = \"http://127.0.0.1:8000/api/v1/todo-list/\" + this.$store.state.selectedStatus + \"/asc/\";\n      } else {\n        if (this.$store.state.selectedOrder != \"\") this.link = \"http://127.0.0.1:8000/api/v1/todo-list/\" + this.$store.state.selectedOrder + \"/\";else this.link = \"http://127.0.0.1:8000/api/v1/todo-list/\";\n      }\n      axios.get(this.link).then(response => {\n        this.todos = response.data;\n      }).catch(err => {\n        alert(\"error fetching list\"), console.log(err);\n      });\n    },\n    'selectedID': function () {\n      if (this.selectedID == \"\") this.link = \"http://127.0.0.1:8000/api/v1/todo-list/\";else this.link = \"http://127.0.0.1:8000/api/v1/todo-list/view/\" + this.selectedID + \"/\";\n      axios.get(this.link).then(response => {\n        this.todos = response.data;\n      }).catch(err => {\n        alert(\"error fetching list\"), console.log(err);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","useModal","name","setup","confirm","onButtonClick","then","ok","console","log","selectedId","data","todos","next","prev","link","methods","deleteIt","delete","alert","catch","err","response","setNe